def application = "ansible-playbook"
pipeline {
  agent any
  environment {
    DOCKERHUB_CREDENTIALS = credentials('dockerhub')
  }
  stages {
       stage('clone') {
      steps {
        git 'git@github.com:AamirKhan511/Ansible_playbook.git'
           sh '''
          #!/bin/bash
          cd /etc/ansible/
          sudo ansible-playbook -i ansible.cfg nginx-playbook.yml -b

          '''
      }
    }
  }
}

